        "properties": {
          "name": {
            "type": "string",
            "description": "Nombre del permiso"
          }
        },
        "required": [
          "name"
        ]
      },
      "UpdatePermissionDto": {
        "type": "object",
        "properties": {
          "originalName": {
            "type": "string",
            "description": "nombre del permiso a actualizar"
          },
          "newName": {
            "type": "string",
            "description": "nuevo nombre del permiso"
          }
        },
        "required": [
          "originalName",
          "newName"
        ]
      }
    }
  }
